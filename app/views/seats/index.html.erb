<h1>Listing seats</h1>

<table>
  <thead>
    <tr>
      <th>District</th>
      <th>Level</th>
      <th>Cycle</th>
      <th>Elected</th>
      <th>Name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @seats.each do |seat| %>
      <tr>
        <td><%= seat.district_id %></td>
        <td><%= seat.level_id %></td>
        <td><%= seat.cycle_id %></td>
        <td><%= seat.elected %></td>
        <td><%= seat.name %></td>
        <td><%= link_to 'Show', seat %></td>
        <td><%= link_to 'Edit', edit_seat_path(seat) %></td>
        <td><%= link_to 'Destroy', seat, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Seat', new_seat_path %>
